[run]
branch = True
source = simplesat

[report]
omit = *test*

# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    def __repr__

    # Don't complain if non-runnable code isn't run:
    if __name__ == .__main__.:

    @abc.abstractmethod
    @abstractmethod
